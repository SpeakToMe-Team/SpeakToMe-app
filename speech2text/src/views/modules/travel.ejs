<div id="yelp" class="row">
    <div class="details col-md-6 col-sm-12">
        <div class="travel" v-for="(travel, key, index) of contentTravel.list">
            <div class="msgVocal hide">
                {{ travel.msgVocal }}
            </div>
            <div class="imgTravel">
                <a v-bind:href="travel.url" target='_blank'>
                <img v-bind:src="travel.image_url" v-bind:alt="travel.name" width='200px'>
            </a>
            </div>
            <div class="infoTravel">
                <address v-html="travel.address"></address>
                Nom : {{ travel.name }}<br /> Note : {{ travel.rating }}<br /> Nombre d'avis : {{ travel.review_count }}<br/>                Cat√©gories : {{ travel.categories }}<br /> {{ travel.address }}<br /> {{ travel.display_phone }}<br />Prix
                : {{ travel.price }}<br />
            </div>

            <div class="mapYelp" class=" col-md-6 col-sm-12">
                <iframe class="iframeYelp" width="100%" height="250" frameborder="0" style="border:0" v-bind:src="travel.addressMap" allowfullscreen>
                </iframe>
            </div>
        </div>
    </div>
</div>