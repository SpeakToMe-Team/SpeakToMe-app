<div class="modal fade" id="settings" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content frame">
                <div class="page-header text-center">
                    <h2 class="title">
                        <span class="fa fa-cog"></span> Settings <button type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                        </button>
                    </h2>
                </div>
                <div class="content">
                    <div>
                        <form method="POST" action="/settings">
                            <div class="form-group">
                                <label for="villeInput">Votre ville par défaut</label>
                                <input v-model="villeInput" aria-describedby="villeHelpBlock" type="text" class="form-control" id="villeInput " placeholder="Ville" value="<%= user.settings.ville %>">
                                <p>Le message est : {{ villeInput }}</p>
                                <p id="villeHelpBlock" class="form-text text-muted">Cela désactive la géolocalisation.</p>
                            </div>                       
                            <div class="form-group">
                                <label for="movieRange">Nombre de résultat Allociné</label>                                
                                <div class="slider-wrapper">
                                    <input v-model="movieRange" type="range" list="moviemarks" class="form-control" id="movieRange" min="1" max="20" step="1" value="<%= user.settings.nbrResultatMovie %>"/>
                                    <p>Nombre de résultat affiché : {{ movieRange }}</p>
                                </div>
                                <datalist id="moviemarks">
                                    <option value="1" label="1">
                                    <option value="2">
                                    <option value="3">
                                    <option value="4">
                                    <option value="5" label="5">
                                    <option value="6">
                                    <option value="7">
                                    <option value="8">
                                    <option value="9">
                                    <option value="10" label="10">
                                    <option value="11">
                                    <option value="12">
                                    <option value="13">
                                    <option value="14">
                                    <option value="15" label="15">
                                    <option value="16">
                                    <option value="17">
                                    <option value="18">
                                    <option value="19">
                                    <option value="20" label="20">                                    
                                </datalist>
                            </div>                            

                            <div class="form-group">
                                <label for="weatherRange">Nombre de résultat Météo</label>                                
                                <div class="slider-wrapper">
                                    <input v-model="weatherRange" type="range" list="weatherMarks" class="form-control" id="weatherRange" min="1" max="20" step="1" value="<%= user.settings.nbrResultatWeather %>"/>
                                    <p>Nombre de résultat affiché : {{ weatherRange }}</p>
                                </div>
                                <datalist id="weatherMarks">
                                    <option value="1" label="1">
                                    <option value="2">
                                    <option value="3">
                                    <option value="4">
                                    <option value="5" label="5">
                                    <option value="6">
                                    <option value="7">
                                    <option value="8">
                                    <option value="9">
                                    <option value="10" label="10">
                                    <option value="11">
                                    <option value="12">
                                    <option value="13">
                                    <option value="14">
                                    <option value="15" label="15">
                                    <option value="16">
                                    <option value="17">
                                    <option value="18">
                                    <option value="19">
                                    <option value="20" label="20">                                    
                                </datalist>
                            </div>

                            <div class="form-group">
                                <label for="travelRange">Nombre de résultat Yelp</label>                                
                                <div class="slider-wrapper">
                                    <input v-model="travelRange" type="range" list="travelMarks" class="form-control" id="travelRange" min="1" max="20" step="1" value="<%= user.settings.nbrResultatTravel %>"/>
                                    <p>Nombre de résultat affiché : {{ travelRange }}</p>
                                </div>
                                <datalist id="travelMarks">
                                    <option value="1" label="1">
                                    <option value="2">
                                    <option value="3">
                                    <option value="4">
                                    <option value="5" label="5">
                                    <option value="6">
                                    <option value="7">
                                    <option value="8">
                                    <option value="9">
                                    <option value="10" label="10">
                                    <option value="11">
                                    <option value="12">
                                    <option value="13">
                                    <option value="14">
                                    <option value="15" label="15">
                                    <option value="16">
                                    <option value="17">
                                    <option value="18">
                                    <option value="19">
                                    <option value="20" label="20">                                    
                                </datalist>
                            </div>

                            <div class="form-group">
                                <label for="spotifyRange">Nombre de résultat Spotify</label>
                                <div class="slider-wrapper">
                                    <input v-model="spotifyRange" type="range" list="spotifyMarks" class="form-control" id="spotifyRange" min="1" max="20" step="1" value="<%= user.settings.nbrResultatSpotify %>"/>
                                    <p>Nombre de résultat affiché : {{ spotifyRange }}</p>
                                </div>
                                <datalist id="spotifyMarks">
                                    <option value="1" label="1">
